import os
import sys
from sourcery.sourceBrowser import SourceBrowser

if len(sys.argv) < 2:
    print "Run: % sourcery_build_cache <sourcery.config>"
else:
    
    configFileName=sys.argv[1].replace("\n", "")
    sb=SourceBrowser(configFileName)
    sb.preprocess()
    